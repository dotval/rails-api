<h1>これまでデータベースに登録された本</h1>
<div style="margin-bottom: 20px;">
  <%= link_to "戻る", root_path %>
</div>
<table border="1" style="text-align: justify; text-justify: inter-ideograph;">
  <thead>
    <tr>
      <th>google_id</th>
      <th>タイトル</th>
    </tr>
  </thead>
  <tbody>
    <% @books.each do |book| %>
      <tr>
        <td><%= book.google_id %></td>
        <td><%= book.title %></td>
      </tr>
    <% end %>
  </tbody>
</table>